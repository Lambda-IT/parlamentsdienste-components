{
    "name": "componentlibrary",
    "version": "1.0.0",
    "description": "Stencil Component Starter",
    "main": "dist/index.cjs.js",
    "module": "dist/index.js",
    "es2015": "dist/esm/index.mjs",
    "es2017": "dist/esm/index.mjs",
    "types": "dist/types/index.d.ts",
    "collection": "dist/collection/collection-manifest.json",
    "collection:main": "dist/collection/index.js",
    "unpkg": "dist/componentlibrary/componentlibrary.js",
    "files": [
        "dist/",
        "loader/"
    ],
    "scripts": {
        "build": "stencil build --docs",
        "b": "npm run build",
        "start": "stencil build --dev --watch --serve",
        "test": "stencil test --spec --e2e",
        "t": "npm run test",
        "test.watch": "stencil test --spec --e2e --watchAll",
        "generate": "stencil generate",
        "g": "npm run generate",
        "storybook": "start-storybook -s ./dist,./src",
        "s": "npm run storybook",
        "build-storybook": "build-storybook -c .storybook-deploy",
        "bs": "npm run build-storybook"
    },
    "devDependencies": {
        "@babel/core": "^7.9.0",
        "@stencil/core": "^2.3.0",
        "@stencil/sass": "^1.4.1",
        "@storybook/addon-actions": "^6.1.11",
        "@storybook/addon-controls": "^6.1.11",
        "@storybook/addon-notes": "^5.3.21",
        "@storybook/addon-viewport": "^6.1.11",
        "@storybook/addons": "^6.1.11",
        "@storybook/html": "^6.1.11",
        "@types/jest": "^25.2.1",
        "@types/puppeteer": "2.0.1",
        "babel-loader": "^8.1.0",
        "bootstrap": "^4.4.1",
        "jest": "^25.3.0",
        "jest-cli": "26.0.1",
        "prettier": "2.0.5",
        "puppeteer": "2.1.1",
        "sass": "^1.32.0"
    },
    "license": "MIT",
    "dependencies": {
        "@a11y/focus-trap": "^1.0.5",
        "@popperjs/core": "^2.4.0",
        "flatpickr": "^4.6.3",
        "resize-observer-polyfill": "^1.5.1"
    }
}
