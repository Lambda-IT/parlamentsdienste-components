{
    "name": "@parlamentsdienste-components/core",
    "version": "0.0.1",
    "description": "Stencil Component Starter",
    "main": "dist/index.cjs.js",
    "module": "dist/index.js",
    "types": "dist/types/index.d.ts",
    "collection": "dist/collection/collection-manifest.json",
    "collection:main": "dist/collection/index.js",
    "unpkg": "dist/core/core.esm.js",
    "typesVersions": {
        "*": {
            "loader": [
                "loader/index.d.ts"
            ],
            "components": [
                "components/index.d.ts"
            ],
            "components/*.js": [
                "components/*.d.ts"
            ]
        }
    },
    "exports": {
        "./loader": {
            "import": "./loader/index.js",
            "require": "./loader/index.cjs"
        },
        "./components": {
            "import": "./components/index.js",
            "require": "./components/index.cjs"
        },
        "./components/*.js": {
            "import": "./components/*.js"
        },
        "./styles/*.css": {
            "import": "./dist/parlamentsdienstecore/*.css"
        },
        "./utils": {
            "import": "./src/utils/index.ts"
        }
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/stenciljs/component-starter.git"
    },
    "files": [
        "dist/",
        "loader/",
        "components/"
    ],
    "scripts": {
        "build": "stencil build",
        "start": "stencil build --dev --watch --serve",
        "test": "stencil test --spec --e2e",
        "test.watch": "stencil test --spec --e2e --watchAll",
        "generate": "stencil generate"
    },
    "dependencies": {
        "@a11y/focus-trap": "^1.0.5",
        "@juggle/resize-observer": "^3.4.0",
        "@lottiefiles/lottie-player": "^2.0.12",
        "@popperjs/core": "^2.11.8",
        "@stencil/sass": "3.0.7",
        "@stencil/store": "^2.1.3",
        "bootstrap": "^4.4.1",
        "flatpickr": "^4.6.13"
    },
    "devDependencies": {
        "@parlamentsdienste-components/angular-output-target": "0.0.1",
        "@stencil/core": "^4.27.1",
        "@stencil/vue-output-target": "^0.10.8",
        "@types/jest": "^29.5.14",
        "@types/node": "^22.13.5",
        "jest": "^29.7.0",
        "jest-cli": "^29.7.0",
        "puppeteer": "^24.3.0"
    },
    "license": "MIT"
}
