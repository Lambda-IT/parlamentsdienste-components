{
    "name": "docs",
    "$schema": "../../node_modules/nx/schemas/project-schema.json",
    "tags": [],
    "targets": {
        "start": {
            "executor": "nx:run-commands",
            "options": {
                "cwd": "{projectRoot}",
                "command": "npx storybook dev -p 6006"
            }
        },
        "build": {
            "executor": "nx:run-commands",
            "dependsOn": [{ "projects": ["core"], "target": "build" }],
            "options": {
                "cwd": "{projectRoot}",
                "command": "npx storybook build"
            }
        }
    }
}
